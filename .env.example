# OpenAI API (для GPT-4 Vision структурирования)
OPENAI_API_KEY=your_api_key_here

# Google Cloud Vision API (альтернатива)
GOOGLE_CLOUD_VISION_API_KEY=your_api_key_here

# AWS Credentials (для Textract)
AWS_ACCESS_KEY_ID=your_key
AWS_SECRET_ACCESS_KEY=your_secret

# OCR Settings
OCR_PROVIDER=tesseract # или 'google', 'aws', 'azure'
OCR_LANGUAGE=eng+rus # поддержка английского и русского
OCR_DPI=300
OCR_CONFIDENCE_THRESHOLD=60
