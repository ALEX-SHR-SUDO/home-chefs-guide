{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "ocr-output",
    "dist"
  ]
}
